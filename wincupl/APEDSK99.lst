  9:Device   G16V8A ;
 10:
 11:
 12:/* input pins */
 13:PIN  2 = AD_A13 ;                                       /* TMS9900  A0                                          */
 14:PIN  3 = AD_A14 ;                                       /* TMS9900  A1                                          */
 15:PIN  4 = AD_A15 ;                                       /* TMS9900  A2                                          */
 16:PIN  5 = AD_A0 ;                                        /* TMS9900 A15                                          */
 17:PIN  6 = WE ;                                           /* TMS9900 WE*                                          */
 18:PIN  7 = MEMEN ;                                        /* TMS9900 MEMEN*                                               */
 19:PIN  8 = TI_BUF ;                                       /* Arduino TIBUF (enables 74HCT541's and GAL16V8)       */
 20:
 21:/* output pins */
 22:PIN 12 = AD_CE ;                                        /* GAL generated chip select for IS61C512               */
 23:PIN 13 = TI_INT ;                                       /* GAL generated Arduino interrupt for DSR write        */
 24:PIN 14 = AD_WE ;                                        /* GAL copy of TMS9900 WE* (aka tri-state buffer)       */
 25:PIN 15  = CE_245 ;                                      /* GAL generated chip select for 74LS245                        */
 26:
 27:/* variables */
 28:LOWMEM  = !MEMEN & !AD_A15 & !AD_A14 &  AD_A13 ;        /* 001: low memory expansion 2000-3FFF                          */
 29:DSR     = !MEMEN & !AD_A15 &  AD_A14 & !AD_A13 ;        /* 010: DSR memory                                              */
 30:HIMEM1  = !MEMEN &  AD_A15 & !AD_A14 &  AD_A13 ;        /* 101: high memory expansion A000-BFFF                         */
 31:HIMEM2  = !MEMEN &  AD_A15 &  AD_A14 & !AD_A13 ;        /* 110: high memory expansion C000-DFFF                         */
 32:HIMEM3  = !MEMEN &  AD_A15 &  AD_A14 &  AD_A13 ;        /* 111: high memory expansion E000-FFFF                         */
 33:
 34:/* equations */
 35:!AD_CE, !CE_245  = LOWMEM # DSR # HIMEM1 # HIMEM2 # HIMEM3;
 36:AD_CE.OE  = !TI_BUF ;
 37:CE_245.OE = !TI_BUF ;
 38:
 39:AD_WE    = WE ;
 40:AD_WE.OE = !TI_BUF ;                                    /* WE* from TMS9900                                             */
 41:
 42:!TI_INT  = DSR & !WE & AD_A0 ;                  /* write to DSR Arduino interrupt                       */
 43:TI_INT.OE = !TI_BUF ; 
